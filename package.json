{
  "name": "cursor-ide-free-trial",
  "version": "1.2.5",
  "main": "src/main/index.js",
  "scripts": {
    "start": "electron-forge start",
    "start:win": "chcp 65001>nul && electron .",
    "dev": "cross-env NODE_ENV=development electron .",
    "dev:win": "chcp 65001>nul && cross-env NODE_ENV=development electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node scripts/build.js",
    "build:dir": "node scripts/build.js --dir",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "dist": "electron-builder",
    "dist:win": "electron-builder --win"
  },
  "build": {
    "appId": "org.trial.free.ide.ai",
    "productName": "Cursor_IDE_Free_Trial",
    "directories": {
      "output": "release"
    },
    "files": [
      "src/**/*",
      "public/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "views",
        "to": "views",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist_public",
        "to": "public",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist_server",
        "to": "server",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "extensions",
        "to": "extensions",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist_package.json",
        "to": "package.json"
      }
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/assets/logo.png"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "portable": {
      "artifactName": "CursorIDEFreeTrial.exe"
    },
    "asar": true
  },
  "keywords": [],
  "author": "豚鼠特攻",
  "license": "ISC",
  "description": "Cursor IDE 免费试用工具",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "cross-env": "^7.0.3",
    "electron": "^33.0.0",
    "electron-builder": "^26.0.12",
    "javascript-obfuscator": "^4.1.1",
    "nan": "^2.22.1",
    "node-gyp": "^11.1.0",
    "nodemon": "^3.0.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  }
}
